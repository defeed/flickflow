!!!
%html
  %head
    %title= title
    = stylesheet_link_tag    'application', 'jasny-bootstrap.min', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'application', 'jasny-bootstrap.min', 'data-turbolinks-track' => true
    %meta{name: "viewport", content: "width=device-width, initial-scale=1, user-scalable=no, minimal-ui"}/
    = csrf_meta_tags
  %body
    = render 'shared/navbar'
    = render 'shared/flash_messages'
    .container-fluid
      = yield
