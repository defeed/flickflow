!!!
%html
  %head
    %title= title
    = stylesheet_link_tag    'application', 'jasny-bootstrap.min'
    = javascript_include_tag 'application', 'jasny-bootstrap.min'
    = javascript_include_tag 'http://cdn.sublimevideo.net/js/cj1d4rzp.js'
    %meta{name: "viewport", content: "width=device-width, initial-scale=1, user-scalable=no, minimal-ui"}/
    = csrf_meta_tags
  %body
    = render 'shared/navbar'
    = render 'shared/flash_messages'
    .container-fluid
      = yield
